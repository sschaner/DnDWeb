@model IEnumerable<DnDWeb.Models.ApiItem>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Character Classes</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Click for Class Details
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.ActionLink(item.name, "Details", new { index=item.index })
            </td>
        </tr>
}
    </tbody>
</table>
